<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acacyc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/lo"
        android:label="@string/app_name"
        android:theme="@style/Theme.MyTheme" >
        <activity
            android:name="com.example.acacyc.splashscreen"
            android:label="@string/app_name"
            android:screenOrientation="landscape"
            android:launchMode="singleTop"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
        	android:name="com.example.acacyc.MainActivity"
        	android:screenOrientation="landscape"
        	>
        </activity>
        <activity
        	android:name="com.example.acacyc.login"
        	android:screenOrientation="landscape"
        	>
        </activity>
        <activity
        	android:name="com.example.acacyc.informacion"
        	android:screenOrientation="landscape"
        	>
        </activity>
        <activity
        	android:name="com.example.acacyc.conocenos"
        	android:screenOrientation="landscape"
        	>
        </activity>
        <activity
        	android:name="com.example.acacyc.saldo"
        	android:screenOrientation="landscape"
        	>
        </activity>
         <activity
        	android:name="com.example.acacyc.credito"
        	android:screenOrientation="landscape"
        	>
        </activity>
        <activity
        	android:name="com.example.acacyc.Tipo"
        	android:screenOrientation="landscape"
        	>
        </activity>
    </application>

</manifest>